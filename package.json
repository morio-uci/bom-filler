{
  "name": "bom-filler",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "apollo-boost": "^0.4.7",
    "apollo-cache-inmemory": "^1.6.5",
    "bcrypt": "^5.0.0",
    "bootstrap": "^4.4.1",
    "connect-session-knex": "https://github.com/machineghost/connect-session-knex.git",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "knex": "^0.20.11",
    "nodemailer": "^6.4.4",
    "octopart": "github:jorisa/octopart",
    "pg": "^7.18.1",
    "react": "^16.8.6",
    "react-apollo": "^3.1.3",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-datasheet": "^1.4.0",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.4.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-integer": "^0.1.0",
    "concurrently": "^4.1.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "mocha": "^7.0.1",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "bash bin/dev.sh",
    "build": "react-scripts build",
    "test-react": "react-scripts test",
    "eject-www": "react-scripts eject",
    "make-db": "set -a && . ./.env; dropdb --if-exists $POSTGRES_DB; createdb $POSTGRES_DB",
    "make-test-db": "set -a && . ./.env; dropdb --if-exists $POSTGRES_TEST_DB; createdb $POSTGRES_TEST_DB",
    "migrate": "knex --esm migrate:latest",
    "test-migrate": "NODE_ENV=test knex --esm migrate:latest",
    "seed": "knex --esm seed:run",
    "test-seed": "NODE_ENV=test knex --esm seed:run",
    "reset-db": "npm run make-db; npm run migrate; npm run seed",
    "reset-test-db": "npm run make-test-db; npm run test-migrate; npm run test-seed",
    "test": "NODE_ENV=test mocha --exit --recursive -r esm",
    "db-test": "npm run reset-test-db; npm run test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:4000"
}
